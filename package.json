{
  "name": "playwright-typescipt-playwright-test",
  "version": "1.0.0",
  "description": "Playwright Framework with Typescript",
  "main": "index.js",
  "author": "sgiri",
  "license": "MIT",
  "scripts": {
    "test:single:Smoke": "npx playwright test /tests/functional/navigateDifferentPages --project=Smoke --workers=1",
    "test:single:chrome2": "npx playwright test /tests/functional/cc/synthetic/VerifyUserCantDeactivateFolder.test.ts --project=Chrome --workers=1",
    "test:single:chrome3": "npx playwright test /tests/functional/cc/synthetic/CreateNewSyntheticProduct.test.ts --project=Chrome --workers=1",
    "test:single:chrome4": "npx playwright test /tests/functional/cc/synthetic/CCNavigationToAndFrom.test.ts --project=Chrome --workers=1",
   "test:single:chromesmoke": "npx playwright test /tests/functional/cc/synthetic/ --project=ChromeSmoke --workers=1",
    "test:single:edge": "npx playwright test /tests/functional/catchpointEndpointDB.test.ts --project=Edge --workers=1",
    "test:single:firefox": "npx playwright test /tests/functional/catchpointEndpointDB.test.ts --project=Firefox --workers=1",
    "test:single:chromeTimeCapture": "npx playwright test /tests/functional/catchointSampleTestForTimeComapre.test.ts --project=Chrome --workers=1",
    "test:single:nav": "npx playwright test /tests/functional/navigateDifferentPages --project=Smoke --workers=1"
    
  },
  "dependencies": {
    "@playwright/test": "^1.29.1",
    "@types/adm-zip": "^0.5.0",
    "@types/crypto-js": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "adm-zip": "^0.5.9",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "eslint": "^8.13.0",
    "eslint-plugin-import": "^2.26.0",
    "exceljs": "^4.3.0",
    "experimental-allure-playwright": "0.0.3",
    "install": "^0.13.0",
    "pg": "^8.7.3",
    "playwright": "^1.29.1",
    "typescript": "^4.6.3"
  }
}
